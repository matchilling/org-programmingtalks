    "gatsby-transformer-sharp": "^1.6.24",
    "gh-pages": "^1.1.0",
    "moment": "^2.22.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-youtube": "^7.6.0",
    "redux": "^4.0.0",
    "uuid": "^3.2.1"
  },
  "keywords": [
    "gatsby"
