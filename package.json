  },
  "homepage": "https://www.programmingtalks.org",
  "dependencies": {
    "dotenv": "^5.0.1",
    "gatsby": "^1.9.256",
    "gatsby-link": "^1.6.41",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-sass": "^1.0.26",
